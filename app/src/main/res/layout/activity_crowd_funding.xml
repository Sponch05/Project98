<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="za.co.sponchware.www.connectedyouth.CrowdFundingActivity">

    <ImageButton
        android:id="@+id/getFundingImgBtn"
        android:layout_width="210dp"
        android:layout_height="139dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="89dp" />

    <TextView
        android:id="@+id/getFundTxtV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get Funding"
        tools:layout_editor_absoluteX="89dp"
        tools:layout_editor_absoluteY="72dp" />

    <TextView
        android:id="@+id/helpFundTxtV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Help Fund"
        tools:layout_editor_absoluteX="89dp"
        tools:layout_editor_absoluteY="259dp" />

    <ImageButton
        android:id="@+id/helpFundBtn"
        android:layout_width="209dp"
        android:layout_height="134dp"
        app:srcCompat="@mipmap/ic_launcher_round"
        tools:layout_editor_absoluteX="89dp"
        tools:layout_editor_absoluteY="276dp" />

</android.support.constraint.ConstraintLayout>
